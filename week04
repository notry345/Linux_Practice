print "<BR>";
print $member[0]["age"];
print "<BR>";
print $member[0]["tall"];
?>

#Operator1.php
<?php
$answer=5+1;
print"5 + 1 = $answer";
print "<BR>";
$answer=5-2;
print"5 - 2 = $answer";
print "<BR>";
$answer=5*3;
print"5 * 3 = $answer";
print "<BR>";
$answer=5/5;
print"5 / 5 = $answer";
print "<BR>";
$answer=5%3;
print"5 % 1 = $answer";
print "<BR>";?>

